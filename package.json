		"@types/node": "^10.12.21",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"vscode-test": "^1.0.0-next.0",
		"@types/vscode": "^1.36.0"
	},
	"dependencies": {
		"universalify": "^0.1.2",
		"jsonfile": "^5.0.0",
		"typescript": "^3.3.1"
