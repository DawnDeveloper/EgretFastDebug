{
	"name": "egret-fast-debug",
	"displayName": "Egret Fast Debug",
	"description": "A tool for Egret project which makes debug faster",
	"version": "1.0.0",
	"publisher": "yaoduoHu",
	"icon": "icon/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/huyaoduo/EgretFastDebug"
	},
	"engines": {
		"vscode": "^1.47.3"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:egretProperties.json",
		"onCommand:extension.egretFastDebug"
